    "fathom-web": "2.8.0",
    "lodash.maxby": "4.6.0",
    "lodash.minby": "4.6.0",
    "lodash.orderby": "4.6.0",
    "prop-types": "15.6.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
